{
  "name": "labelify",
  "version": "1.0.0",
  "description": "Automate and simplify the creation of labels for your repository.",
  "keywords": [
    "label",
    "labels",
    "git",
    "repo",
    "github",
    "gitlab",
    "utils"
  ],
  "author": "Itgalaxy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/itgalaxy/labelify.git"
  },
  "homepage": "https://github.com/itgalaxy/labelify",
  "bugs": "https://github.com/itgalaxy/labelify/issues",
  "main": "src/index.js",
  "bin": "bin/labelify.js",
  "dependencies": {
    "meow": "^5.0.0",
    "resolve-from": "^4.0.0",
    "cosmiconfig": "^5.0.0",
    "got": "^8.3.0",
    "pkg-up": "^3.0.1",
    "hosted-git-info": "^2.7.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "del-cli": "^1.1.0",
    "eslint": "^5.15.3",
    "eslint-plugin-ava": "^6.0.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-itgalaxy": "^99.0.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-unicorn": "^7.1.0",
    "execa": "^1.0.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "nock": "^10.0.6",
    "npm-run-all": "^4.0.1",
    "npmpub": "^4.1.0",
    "prettier": "^1.5.2",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-itgalaxy": "^14.0.0"
  },
  "scripts": {
    "labelify": "node ./bin/labelify --overlap",
    "lint:prettier": "prettier --list-different '{bin,src,__tests__}/**/*.{js,mjs,jsx,md,yml}' '*.{js,mjs,jsx,md,yml}'",
    "lint:eslint": "eslint --cache --report-unused-disable-directives . --ignore-path .gitignore --ext 'js,.mjs,.jsx,.md'",
    "lint:remark": "remark . -i .gitignore -f -q",
    "lint": "npm-run-all -l -p lint:**",
    "prettify": "npm run lint:prettify -- --write",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix": "npm-run-all -l -p 'fix:**'",
    "pretest": "npm run lint",
    "test:only": "jest --coverage --testPathIgnorePatterns fixtures",
    "test": "npm run test:only",
    "release": "npmpub"
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "script"
    },
    "extends": [
      "plugin:itgalaxy/node",
      "plugin:itgalaxy/esnext",
      "plugin:itgalaxy/jest",
      "plugin:itgalaxy/markdown"
    ],
    "overrides": [
      {
        "env": {
          "browser": true,
          "node": true
        },
        "files": [
          "**/*.md"
        ],
        "settings": {
          "node": {
            "allowModules": [
              "labelify"
            ]
          }
        },
        "rules": {
          "strict": "off",
          "no-process-env": "off",
          "no-console": "off",
          "import/no-unresolved": [
            "error",
            {
              "commonjs": true,
              "caseSensitive": true,
              "ignore": [
                "^labelify$"
              ]
            }
          ]
        }
      },
      {
        "files": [
          "**/__tests__/**/*.js"
        ],
        "rules": {
          "node/no-unsupported-features": "off"
        }
      }
    ],
    "root": true
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-itgalaxy"
    ]
  },
  "engines": {
    "node": ">= 8.9.0"
  },
  "files": [
    "bin",
    "src",
    "!**/__tests__/**"
  ]
}
